[sub_resource type="GDScript" id="GDScript_trvmb"]
resource_name = "Player"
script/source = "extends Area2D

var weapon = 'rocket'
var engine = 'base'
var canShoot = true
var shoots = true
var fireRate = 0.5
var bulletSpeed = 200
var speed = 240
var maxHealth = 4
var health = 4
var damage = 1

func _ready() -> void:
	$ship.play('default')
	get_node(\"ship/weapons/%s\" % [weapon]).visible = true
	get_node(\"ship/weapons/%s\" % [weapon]).connect(\"animation_finished\", weapon_animation)
	get_node(\"ship/weapons/%s\" % [weapon]).play('default')
	get_node(\"ship/engines/%s\" % [engine]).visible = true
	get_node(\"ship/weapons/%s\" % [weapon]).connect(\"animation_finished\", engine_animation)
	get_node(\"ship/engines/%s\" % [engine]).play('default')
	
func _process(delta: float) -> void:
	follow_mouse(delta)
	$collision.global_position = $ship.global_position
	if Input.is_action_pressed('shoot'):
		shoot()
	if Input.is_action_pressed('weapon1'):
		set_weapon(\"rocket\")
		change_weapon()
	if Input.is_action_pressed('weapon2'):
		set_weapon(\"zap\")
		change_weapon()
	if Input.is_action_pressed('weapon3'):
		set_weapon(\"cannon\")
		change_weapon()
	if Input.is_action_pressed('weapon4'):
		set_weapon(\"biggun\")
		change_weapon()
	if Input.is_action_pressed('engine1'):
		set_engine(\"base\")
		change_engine()
	if Input.is_action_pressed('engine2'):
		set_engine(\"charged\")
		change_engine()
	if Input.is_action_pressed('engine3'):
		set_engine(\"burst\")
		change_engine()
	if Input.is_action_pressed('engine4'):
		set_engine(\"big\")
		change_engine()
	
func follow_mouse(delta: float) -> void:
	var mouse_position = get_global_mouse_position()
	var direction = (mouse_position + Vector2(0, -50) - $ship.global_position).normalized()
	var distance = $ship.global_position.distance_to(mouse_position + Vector2(0, -50))
	
	if distance < 1.5:
		$ship.global_position = mouse_position + Vector2(0, -50)
		return
	$ship.global_position += direction * speed * delta
	var w = get_viewport_rect().size.x
	var h = get_viewport_rect().size.y
	if $ship.global_position.x <= 24:
		$ship.global_position.x = 24
	if $ship.global_position.x >= w - 24:
		$ship.global_position.x = w - 24

	if $ship.global_position.y < 24:
		$ship.global_position.y = 24
	if $ship.global_position.y >= h - 24:
		$ship.global_position.y = h - 24

func shoot():
	if canShoot:
		set_canShoot(false)
		get_tree().create_timer(fireRate).timeout.connect(func(): set_canShoot(true))
		
		get_node(\"ship/weapons/%s\" % [weapon]).play('shooting')
		
		var new_bullet = get_node(\"bullets/%s\" % [weapon]).duplicate()
		new_bullet.position = $ship.position
		new_bullet.visible = true
		new_bullet.set_speed(bulletSpeed)
		new_bullet.monitoring = true
		new_bullet.parent = false
		add_child(new_bullet)
		
func weapon_animation() -> void:
	get_node(\"ship/weapons/%s\" % [weapon]).play('default')
	
func engine_animation() -> void:
	get_node(\"ship/engines/%s\" % [engine]).play('default')
	
func change_engine() -> void:
	$ship/engines/base.visible = false
	$ship/engines/charged.visible = false
	$ship/engines/burst.visible = false
	$ship/engines/big.visible = false
	var node = get_node(\"ship/engines/%s\" % [engine])
	node.visible = true
	node.play('default')
	
func change_weapon() -> void:
	$ship/weapons/biggun.visible = false
	$ship/weapons/zap.visible = false
	$ship/weapons/cannon.visible = false
	$ship/weapons/rocket.visible = false
	var node = get_node(\"ship/weapons/%s\" % [weapon])
	node.visible = true
	node.play('default')
	
func set_fireRate(value: float) -> void:
	fireRate = value
	
func set_bulletSpeed(value: float) -> void:
	bulletSpeed = value
	
func set_speed(value: float) -> void:
	speed = value
	
func set_weapon(value: String) -> void:
	weapon = value
	if ready:
		change_weapon()
	
func set_engine(value: String) -> void:
	engine = value
	if ready:
		change_engine()
	
func set_shield(value: String) -> void:
	pass
	
func set_canShoot(value: bool) -> void:
	canShoot = value
	
func set_shoots(value: bool) -> void:
	shoots = value
	
func set_maxHealth(value: float) -> void:
	maxHealth = value
	
func set_health(value: float) -> void:
	health = value
	
func set_damage(value: float) -> void:
	damage = value

func _on_area_entered(area: Area2D) -> void:
	if \"enemyBullet\" in area.get_groups():
		area.die()
		#takeDamage(area.damage)
	if \"enemy\" in area.get_groups():
		pass
		#takeDamage(area.damage)
		
"

[sub_resource type="GDScript" id="GDScript_6apk8"]
resource_name = "rocket"
script/source = "extends Node2D

var speed = 100
var damage = 1
var parent = true

func _ready() -> void:
	if parent:
		return
	self.visible = true
	$bullet.play('default')

func _process(delta: float) -> void:
	if parent:
		return
	self.position.y -= speed * delta
	if self.global_position.y <= 0:
		self.queue_free()
	
func set_speed(value: float) -> void:
	speed = value
	
func set_damage(value: float) -> void:
	damage = value
	
func set_parent(value: bool) -> void:
	parent = value

func _on_area_entered(area: Area2D) -> void:
	if \"enemy\" in area.get_groups():
		if !area.alive:
			return
		set_parent(true)
		$bullet.play(\"explosion\")
		$bullet.connect(\"animation_finished\", queue_free)
		area.takeDamage(damage)
		
"

